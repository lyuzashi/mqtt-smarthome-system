#cloud-config

# Enable password authentication with the SSH daemon
ssh_pwauth: true
hostname: HAL9000

# Start with user and password default with expiry
chpasswd:
  expire: true
  list:
  - ben:ben

users:
  - name: ben
    groups: sudo, ssh
    ssh_import_id: None
    shell: /bin/bash
    sudo: ALL=(ALL) NOPASSWD:ALL
  - name: git
    shell: /usr/bin/git-shell
    ssh_import_id: None
    sudo: ALL=(hal) NOPASSWD:/usr/bin/git *
    ssh_authorized_keys:
      - no-port-forwarding,no-X11-forwarding,no-agent-forwarding,no-pty ssh-rsa  ben@Number5
  - name: hal
    ssh_import_id: None
    groups: git, audio
    shell: /bin/bash

## Update apt database and upgrade packages on first boot
package_update: true
package_upgrade: true

## Install additional packages on first boot
packages:
- mongodb-org
- nodejs
- linux-tools-common # USB/IP
- linux-tools-raspi2
- build-essential # For compiling
- python3-distutils # For installing pigpio
- libasound2-dev
- alsa-base
- alsa-utils
- bluetooth
- libbluetooth-dev
- libudev-dev
- git
- libcap2-bin
- redis-server
- libavahi-compat-libdnssd-dev
- vlan
- pi-bluetooth

write_files:
- path: /etc/modules
  append: true
  content: |
    vhci-hcd
    8021q
- path: /home/git/mqtt-smarthome-system/hooks/post-update
  permissions: '0744'
  content: |
    #!/bin/bash
    git update-server-info
    DIR=/home/hal/mqtt-smarthome-system
    sudo -u hal git --git-dir $DIR/.git --work-tree $DIR clean -fdx
    sudo -u hal git --git-dir $DIR/.git --work-tree $DIR reset --hard HEAD
    sudo -u hal git --git-dir $DIR/.git --work-tree $DIR pull
- path: /home/git/mqtt-smarthome-system/config
  permissions: '0644'
  content: |
    [core]
      repositoryformatversion = 0
      filemode = true
      bare = true
      ignorecase = true
      precomposeunicode = true
    [http]
      receivepack = true
- path: /home/hal/mqtt-smarthome-system/.git/config
  permissions: '0644'
  content: |
    [core]
      repositoryformatversion = 0
      filemode = true
      bare = false
      logallrefupdates = true
    [remote "origin"]
      url = /home/git/mqtt-smarthome-system
      fetch = +refs/heads/*:refs/remotes/origin/*
    [branch "master"]
      remote = origin
      merge = refs/heads/master
- path: /home/hal/mqtt-smarthome-system/.git/hooks/post-merge
  permissions: '0744'
  content: |
    #!/bin/bash
    yarn install
- path: /etc/systemd/system/pm2.service
  permissions: '0644'
  content: |
    [Unit]
    After=network.target
    [Service]
    User=hal
    Group=hal
    Environment=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/home/hal/.yarn/bin PM2_HOME=/home/hal/.pm2
    ExecStart=/home/hal/.yarn/bin/pm2 resurrect --no-daemon
    ExecReload=/home/hal/.yarn/bin/pm2 reload all
    ExecStop=/home/hal/.yarn/bin/pm2 kill
    [Install]
    WantedBy=multi-user.target
- path: /etc/mongod.conf
  content: |
    storage:
      dbPath: /var/lib/mongodb
      journal:
        enabled: true
    systemLog:
      destination: file
      logAppend: true
      path: /var/log/mongodb/mongod.log
    net:
      port: 27017
      bindIp: 192.168.1.86
    processManagement:
      timeZoneInfo: /usr/share/zoneinfo

bootcmd:
  - ip link add link eth0 name eth0.6 type vlan id 6

runcmd:
  - sed -i 's/obscure//g' /etc/pam.d/common-password
  - apt-get remove cmdtest yarn -y
  - apt-get update
  - apt-get install yarn -y
  - setcap cap_net_bind_service,cap_net_raw+eip $(eval readlink -f `which node`)
  - modprobe vhci-hcd
  - chown -R hal /home/hal
  - git init /home/hal/mqtt-smarthome-system
  - git init /home/git/mqtt-smarthome-system --bare
  - chown -R hal:git /home/hal/mqtt-smarthome-system
  - chown -R git:git /home/git/mqtt-smarthome-system
  - chmod -R g+w /home/hal/mqtt-smarthome-system
  - sudo -u git git -C /home/git/mqtt-smarthome-system fetch -q https://github.com/lyuzashi/mqtt-smarthome-system.git master:master
  - cd /home/git/mqtt-smarthome-system
  - sudo -u git /home/git/mqtt-smarthome-system/hooks/post-update
  - cd /home/hal/mqtt-smarthome-system
  - sudo -u hal .git/hooks/post-merge
  - sudo -u hal yarn global add pm2
  - git clone --branch V72 --depth 1 https://github.com/joan2937/pigpio.git
  - cd pigpio
  - make 
  - make install
  - pigpiod
  - echo 'NODE_ENV=production' | sudo -u hal tee -a /home/hal/.profile # permission failed
  - echo 'PATH="/home/hal/.yarn/bin:$PATH"' | sudo -u hal tee -a /home/hal/.profile
  - service mongod start
  - systemctl start pm2

